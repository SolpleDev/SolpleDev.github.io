            "description": "Error message"
          },
          "status": {
            "type": "string",
            "description": "Error name"
          },
          "code": {
            "type": "integer",
            "description": "Error code"
          },
          "errors": {
            "type": "object",
            "description": "Errors"
          }
        }
      },
      "PaginationMetadata": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer"
          },
          "total_pages": {
            "type": "integer"
          },
          "first_page": {
            "type": "integer"
          },
          "last_page": {
            "type": "integer"
          },
          "page": {
            "type": "integer"
          },
          "previous_page": {
            "type": "integer"
          },
          "next_page": {
            "type": "integer"
          }
        }
      },
      "PlainUser": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "id": {
            "type": "integer",
            "readOnly": true
          }
        },
        "required": [
          "email"
        ]
      },
      "School": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "code": {
            "type": "integer"
          },
          "address": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "website": {
            "type": "string"
          },
          "state_code": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "users": {
            "type": "array",
            "readOnly": true,
            "items": {
              "$ref": "#/components/schemas/PlainUser"
            }
          },
          "zip": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          }
        },
        "required": [
          "address",
          "city",
          "code",
          "name",
          "phone",
          "state",
          "state_code",
          "type",
          "website",
          "zip"
        ]
      },
      "Subject": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "subject_id": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "subject_reward": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "readOnly": true
          }
        },
        "required": [
          "category",
          "name",
          "subject_id",
          "subject_reward"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "password": {
            "type": "string",
            "writeOnly": true
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "PlainSubject": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
